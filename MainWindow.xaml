<Window x:Class="Viikkotehtava4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Viikkotehtava4"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" x:Name="mainWindow">
    <Grid>
        <Menu x:Name="menu" HorizontalAlignment="Left" Height="auto" VerticalAlignment="Top" Width="517">
            <MenuItem Name="menuFile" Header="_File">
                <MenuItem Command="ApplicationCommands.New"/>
            </MenuItem>
        </Menu>
        <ScrollViewer Margin="0,21,0,0">
            <WrapPanel Name="WrapPanelTreenit" Width="auto" Height="auto">
                <local:Treeni/>
            </WrapPanel>
        </ScrollViewer>
    </Grid>
</Window>
